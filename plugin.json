{
    "id": "jitsi",
    "name": "Jitsi",
    "description": "Jitsi audio and video conferencing plugin for Mattermost.",
    "version": "0.1.1",
    "backend": {
        "executable": "server/plugin.exe"
    },
    "webapp": {
        "bundle_path": "/static/jitsi_bundle.js"
    },
    "settings_schema": {
        "settings": [
            {
                "key": "JitsiURL",
                "display_name": "Jitsi Server URL",
                "type": "text",
                "help_text": "The URL for an on-premise Jitsi server. For example, https://jitsi.example.com.",
                "placeholder": "https://jitsi.example.com"
            }
        ]
    }
}
